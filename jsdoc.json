{
    "source": {
        "include": [
            "./src/components/", "src/components/SendMessage/"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "exludePattern": "(node_modules|docs)",
        "recurseDepth": 2
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "encoding": "utf8",
        "readme": "./readme.md",
        "destination": "docs/",
        "recurce": true,
        "verbose": true
    }
}